pipeline {
    agent any
    triggers {
        cron('H */4 * * 1-5')
    }
    stages {
        stage('Example') {
            steps {
                echo 'Hello World'
    }
    stages {
        stage('Build') {
            node {
                echo 'Building'
    }
    stages {
        stage('Test') {
            node {
                echo 'Testing'
            }
        }
    }  
